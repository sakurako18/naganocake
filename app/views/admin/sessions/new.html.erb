<div class="container">
  <div class="row">
      <div class="col-xs-9 col-xs-offset-1">
      <h3>管理者ログイン</h3>
  <table class=table-borderess>

      <%=form_with model: @admin,url: admin_session_path, method: :post do |f| %>
      <tr>
        <td><%= f.label :"メールアドレス", class: "mr-3" %></td>
        <td><%= f.text_field :email, autofocus: true, class: "mr-5" %></td>
      </tr>
      <tr>
        <td><%= f.label :"パスワード", class: "mr-3" %></td>
        <td><%= f.text_field :password, autocomplete: "current-password", class: "mr-5" %></td>
      </tr>
    <div class="row">
        <td><%= f.submit'ログイン',class: "btn btn-primary" %></td>
      <% end %>
    </tr>
  </table>
