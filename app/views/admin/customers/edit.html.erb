  <h3><%= customer.last_name+first_name %>さんの会員情報</h3>

    <% @customers.each do |item| %>

    <tr>
      <td>会員ID</td>
      <td><%= customer.1d %></td>
    </tr>
    <tr>
      <td>氏名</td>
      <td><%= customer.last_name %> <%= customer.first_name %></td>
    </tr>
    <tr>
      <td>フリガナ</td>
      <td><%= customer.last_name_kana %> <%= customer.first_name_kana %></td>
    </tr>
    <tr>
      <td>郵便番号</td>
      <td><%= customer.postal_code %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= customer.address %></td>
    </tr>
    <tr>
      <td>電話番号</td>
      <td><%= customer.telephone_number %></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%= customer.postal_code %></td>
    </tr>
    <tr>
      <td>会員ステータス</td>
      <td><%= customer.is_deleted %></td>
    </tr>

    <%= f.submit "編集する", class: "btn btn-success" %>
    <%= f.submit "注文履歴一覧を見る", class: "btn btn-primary" %>